{
	"name": "rive-playground",
	"version": "1.4.5",
	"description": "Interactive Rive file parser, inspector, and debugging tool",
	"type": "module",
	"scripts": {
		"dev": "npx http-server . -p 8080 -o",
		"test-deployment": "node scripts/test-deployment.js",
		"version:patch": "scripts/version.sh patch",
		"version:minor": "scripts/version.sh minor",
		"version:major": "scripts/version.sh major",
		"version:check": "echo \"Current version: $(node -p 'require(\\\"./package.json\\\").version')\"",
		"release": "echo \"Use 'npm run version:patch|minor|major \\\"commit message\\\"' to create a release\""
	},
	"dependencies": {
		"@antv/g6": "^5.0.48",
		"@fortawesome/fontawesome-free": "^6.7.2",
		"@rive-app/webgl2": "^2.27.2",
		"flatted": "^3.3.3",
		"golden-layout": "^1.5.9",
		"jquery": "^3.6.0",
		"jsoneditor": "^10.2.0",
		"serialize-javascript": "^6.0.2"
	}
}
